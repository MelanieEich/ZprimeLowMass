/* Here the uncertainties for the covariance matrix are set.
   The uncertainties come in principle from:  cmssw/TopQuarkAnalysis/TopObjectResolutions/python/stringResolutions_etEtaPhi_Fall11_cff.py */



//Resolution of the Muons to set up covariance matrix
class MuonUncertainties {

 public:
  enum Flavor {kUds, kB};
    
  MuonUncertainties(){};
  ~MuonUncertainties(){};
  
  inline double et(double et, double eta,  Flavor flav);
  inline double eta(double et, double eta,  Flavor flav);
  inline double phi(double et, double eta,  Flavor flav);
};

inline double MuonUncertainties::et(double et, double eta,  Flavor flav)
{
  double res = 1000.; 

  return res;
}


inline double MuonUncertainties::eta(double et, double eta,  Flavor flav)
{
  double res = 1000.; 

  return res;
}


inline double MuonUncertainties::phi(double et, double eta,  Flavor flav)
{
  double res = 1000.;

  return res;
}
